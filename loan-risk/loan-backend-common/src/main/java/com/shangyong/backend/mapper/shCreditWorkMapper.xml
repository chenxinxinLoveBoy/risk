	<!-- 通用查询结果列-->
	<sql id="Base_Column_List">
sh_credit_work_id,	application_id,	work_number,	work_detail,	work_profession,	work_time,	create_time,	create_man,	modify_time,	modify_man,	remark
	</sql>

	<!-- 查询（根据主键ID查询） -->
	<select id="getEntityById" resultMap="shCreditWorkMap" parameterType="java.lang.String">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM sh_credit_work
		 WHERE sh_credit_work_id = #{shCreditWorkId}
	</select>
	<!-- 查询（根据主键ID查询） -->
	<select id="queryByApplicationId" resultMap="shCreditWorkMap" parameterType="java.lang.String">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM sh_credit_work
		 WHERE application_id = #{applicationId}
	</select>

	<!-- 查询（根据主键ID查询） -->
	<select id="findAll" resultMap="shCreditWorkMap" parameterType="java.lang.String">
		 SELECT
		 <include refid="Base_Column_List" />
		 FROM sh_credit_work
		 WHERE sh_credit_work_id = #{shCreditWorkId}
	</select>

	<!--删除：根据主键ID删除-->
	<delete id="deleteEntity" parameterType="java.lang.String">
