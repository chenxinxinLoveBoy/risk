 		(sh_credit_work_id,application_id,work_number,work_detail,work_profession,work_time,create_time,create_man,modify_time,modify_man,remark) 
		 VALUES
 	    <foreach collection="list" item="item" index="index" separator="," >  
 		(#{item.shCreditWorkId},#{item.applicationId},#{item.workNumber},#{item.workDetail},#{item.workProfession},#{item.workTime},#{item.createTime},#{item.createMan},#{item.modifyTime},#{item.modifyMan},#{item.remark}) 
	    </foreach>
	</insert>

